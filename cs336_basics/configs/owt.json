{
    "seed": 32,
    "data": {
        "train": "./data/owt_train.npy",
        "valid": "./data/owt_valid.npy",
        "dtype": "uint16"
    },
    "model": {
        "vocab_size": 32000,
        "context_length": 1024,
        "d_model": 768,
        "num_layers": 8,
        "num_heads": 8,
        "d_ff": 2048,
        "rope_theta": 1e4,
        "device": "cuda"
    },
    "optimizer": {
        "lr": 5e-4,
        "betas": [0.9, 0.95],
        "eps": 1e-8,
        "weight_decay": 0.1
    },
    "train": {
        "batch_size": 8,
        "steps": 2500,
        "lr_max": 4e-3,
        "lr_min": 5e-5,
        "t_w": 200,
        "t_c": 2300,
        "grad_clip": 1.0,
        "accum_steps": 8,
        "save": {
            "enable": true,
            "interval": 50,
            "to": "./dist/ckpt.pt"
        }
    },
    "tokenizer": {
        "vocab_path": "./data/owt_train_vocab.pkl",
        "merges_path": "./data/owt_train_merges.pkl",
        "special_tokens": ["<|endoftext|>"]
    },
    "infer": {
        "kv_cache": false,
        "checkpoint": "./dist/saves/checkpoint_2399_1.64.pt"
    }
}